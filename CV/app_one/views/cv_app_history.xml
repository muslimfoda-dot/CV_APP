<odoo>
   <record id="history_view_list" model="ir.ui.view">
        <field name="name">history list</field>
        <field name="model">history</field>
        <field name="arch" type="xml">
            <list create="0" delete="0" edit="0">
                <field name="old_state"/>
                <field name="new_state"/>
                <field name="user_id"/>
                <field name="app_one"/>
                <field name="reason"/>
            </list>
        </field>
    </record>


    <record id="history_view_form" model="ir.ui.view">
        <field name="name">history form</field>
        <field name="model">history</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group create="0" delete="0" edit="0">
                     <field name="new_state"/>
                     <field name="user_id"/>
                     <field name="app_one"/>
                     <field name="old_state"/>
                     <field name="reason"/>
                    </group>
                         <notebook>
                        <page string="New Page">
                            <field name="line_ids">
                                <list editable="bottom">
                                    <field name="description"/>
                                    <field name="code"/>
                                </list>
                            </field>
                        </page>
                    </notebook>
                </sheet>

            </form>
        </field>
    </record>

    <record id="history_action" model="ir.actions.act_window">
        <field name="name">history action</field>
        <field name="res_model">history</field>
    </record>
</odoo>